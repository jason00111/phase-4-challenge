<div class="container">
  <div class="row navbar">
    <div class="two columns">
      <a href="/">Vinyl</a>
    </div>
    <div class="six columns">
      <a href="/search">Search</a>
    </div>
    <% if (userID) { %>
      <div class="two columns">
        <a href="/users/<%= userID %>">Profile</a>
      </div>
      <div class="two columns">
        <a href="/signout">Sign Out</a>
      </div>
    <% } else { %>
      <div class="two columns">
        <a href="/signup">Sign Up</a>
      </div>
      <div class="two columns">
        <a href="/signin">Sign In</a>
      </div>
    <% } %>
  </div>
</div>
